<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear2"
		android:layout_width="40dp"
		android:layout_height="3dp"
		android:padding="8dp"
		android:background="#FFFFFF"
		android:orientation="horizontal"
		android:layout_gravity="center_horizontal|center_vertical"/>
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="15dp"
		android:layout_marginTop="5dp"
		android:layout_marginRight="15dp"
		android:layout_marginBottom="15dp"
		android:padding="8dp"
		android:background="#FFFFFF"
		android:orientation="vertical">
		<TextView
			android:id="@+id/textview1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Info"
			android:textSize="18sp"
			android:textStyle="bold"
			android:textColor="#000000"/>
		<TextView
			android:id="@+id/textview2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="By ios7jbpro with &lt;3.\nTested by WindowZ414.\nTested by icylemon.\nTested by sertacordar.\nTested by Cotunjr.\nTested by ssSmogg.\nTested by alt1nay.\nTested by MFZengin.\nTested by lipirci19.\nTested by AospkurmayanGay.\nTested by sudo_nautilus.\nTested by Mikey_san7.\nBottomSheetAsActivity by shapun.\n\nAs always:\nSharing without credits:\nDie."
			android:textSize="12sp"
			android:textColor="#000000"/>
		<TextView
			android:id="@+id/textview3"
			android:layout_width="match_parent"
			android:layout_height="45dp"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="Close"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#2196F3"/>
		<TextView
			android:id="@+id/textview4"
			android:layout_width="match_parent"
			android:layout_height="45dp"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="My Telegram"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#2196F3"/>
		<TextView
			android:id="@+id/textview5"
			android:layout_width="match_parent"
			android:layout_height="45dp"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="My YouTube"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#F44336"/>
		<TextView
			android:id="@+id/textview6"
			android:layout_width="match_parent"
			android:layout_height="45dp"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="My Instagram"
			android:textSize="12sp"
			android:textStyle="bold"
			android:textColor="#9C27B0"/>
